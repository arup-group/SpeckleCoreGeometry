    <PackageProjectUrl>https://github.com/speckleworks/SpeckleCoreGeometry</PackageProjectUrl>
    <RepositoryUrl>https://github.com/speckleworks/SpeckleCoreGeometry</RepositoryUrl>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0.0</Version>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <InformationalVersion>1.0.0.0</InformationalVersion>
